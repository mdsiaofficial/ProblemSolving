void solve()
{
	int n;
	cin >> n;
	for(int i = 1; i <= n ; i ++ ) cin >> a[i];
	for(int i = 1; i <= n ; i ++ ) cin >> b[i];
	int idx = n;
	for(int i = n ; i >= 1 ; i -- )
	{
		int t = lower_bound(b + 1, b + n + 1, a[i]) - b;
		ans[i][0] = b[t] - a[i];
		ans[i][1] = b[idx] - a[i];
		if(t == i) idx = i - 1;
	}
	for(int i = 1 ; i <= n ; i ++ ) cout << ans[i][0] << " ";
	cout << endl;
	for(int i = 1 ; i <= n ; i ++ ) cout << ans[i][1] << " ";
	cout << endl;
}
